{
    "_version": "1.12.0",
    "sap.app": {
        "id": "task.shanita",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "invoiceRemote": {
                "uri": "https://services.odata.org/V2/Northwind/Northwind.svc/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "task.shanita.view.App",
            "type": "XML",
            "async": true,
            "id": "fcl"
        },
        "dependencies": {
            "minUI5Version": "1.98.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.table": {},
                "sap.f": {},
                "sap.uxap": {},
                "sap.ui.webc.main": {}
            }
        },
        "config": {
            "fullWidth": true
        },
        
        "routing": {
            "config": {
                "routerClass": "sap.f.routing.Router",
                "type": "View",
                "viewType": "XML",
                "path": "task.shanita.view",
                "controlId": "flexibleColumnLayout",
                "transition": "slide",
                "bypassed": {
                },
                "async": true
            },
            "routes": [
                {
                    "pattern": ":layout:",
                    "name": "master",
                    "target": [
                        "master",
                        "orderForm"
                    ]
                },
                {
                    "pattern": "orderForm/{orderId}/{layout}",
                    "name": "orderForm",
                    "target": [
                        "master",
                        "orderForm"
                    ]
                }
            ],
            "targets": {
                "master": {
                    "name": "OrderList",
                    "controlAggregation": "beginColumnPages"
                },
                "orderForm": {
                    "name": "OrderForm",
                    "controlAggregation": "midColumnPages"
                }
            }
        },
        "models": {
            
        }
    }
}